<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.Etkinlik" table="ETKINLIK">
		<id column="ID" name="id" type="java.lang.Integer" />
		<property column="BASLANGICZAMANI" name="baslangicZamani" type="java.lang.String" />
		<property column="BITISZAMANI" name="bitisZamani" type="java.lang.String" />
		<property column="YASARALIGI" name="yasAraligi" type="string" />
		<property column="EMAIL" name="email" type="java.lang.String" />
		<property column="ISMI" name="etkinlikIsmi" type="java.lang.String" />

	</class>
</hibernate-mapping>

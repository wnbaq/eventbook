<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.Etkinlik" table="ETKINLIK">
		<id column="e_id" name="id" type="java.lang.Integer" />
		<property column="etkinlik_ismi" name="etkinlikIsmi" type="java.lang.String" />
		<property column="baslangic_zamani" name="baslangicZamani" type="java.util.Date" />
		<property column="bitis_zamani" name="bitisZamani" type="java.util.Date" />
		<property column="yas_araligi" name="yasAraligi" type="java.lang.String" />
		<property column="tur" name="type" type="java.lang.String" />
	</class>
</hibernate-mapping>
